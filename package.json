{
  "name": "shighl-vuejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:wc": "npx vue-cli-service build --dest lib --target wc --name shighl-vuejs src/components/LoadShape.vue",
    "lint": "vue-cli-service lint",
    "cpdir": "npx copyfiles public/vendor/* lib/vendor/*",
