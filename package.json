    "postgit": "git push && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@solid/query-ldflex": "^2.11.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.16.0",
